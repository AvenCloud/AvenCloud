{
  "name": "@aven/cloud",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nodemon --watch server -e js,ts,tsx --exec 'ts-node --project tsconfig.server.json lib/server/server.ts'",
    "build": "yarn prisma generate && yarn build:server && yarn build:next",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "production:build": "rm -rf node_modules && yarn && yarn build",
    "production:migrate": "prisma migrate deploy --preview-feature",
    "production:start": "NODE_ENV=production node dist/lib/server/server.js",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "start": "yarn dev",
    "test:integration": "ts-node --project tsconfig.server.json test/run-integration.ts",
    "test:unit": "jest unit",
    "test": "yarn test:unit && yarn lint && yarn build && yarn test:integration",
    "configure-package": "ts-node --project tsconfig.server.json config/configure-package.ts",
    "shell": "yarn configure-package aven-shell && yarn --cwd packages/aven-shell run",
    "client": "yarn configure-package client && yarn --cwd packages/client run",
    "example": "yarn --cwd rocketship-example",
    "migrate-dev": "DATABASE_URL='postgresql://user:pw@localhost:5992/db' yarn prisma migrate dev --preview-feature"
  },
  "dependencies": {
    "@chakra-ui/core": "1.0.0-rc.7",
    "@chakra-ui/icons": "^1.0.0-rc.8",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@expo/spawn-async": "^1.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/pro-light-svg-icons": "^5.15.2",
    "@fortawesome/pro-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@prisma/client": "^2.18.0",
    "@sendgrid/mail": "^7.4.2",
    "@stripe/stripe-js": "^1.13.1",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/minio": "^7.0.7",
    "ajv": "^7.2.1",
    "all-the-cities": "^3.1.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "emotion-theming": "^11.0.0",
    "expo": "^40.0.1",
    "express": "^4.17.1",
    "fast-json-patch": "https://github.com/Starcounter-Jack/JSON-Patch",
    "framer-motion": "^3.10.2",
    "fs-extra": "^9.1.0",
    "json-schema-to-typescript": "^10.1.3",
    "jwt-simple": "^0.5.6",
    "mdx-prism": "^0.3.3",
    "minimist": "^1.2.5",
    "minio": "^7.0.18",
    "next": "10.0.8",
    "next-mdx-enhanced": "^5.0.0",
    "node-fetch": "^2.6.1",
    "nookies": "^2.5.2",
    "open": "^8.0.2",
    "pg": "^8.5.1",
    "pg-format": "^1.0.4",
    "php-serialize": "^4.0.2",
    "pojo-maps": "^0.3.0",
    "prompts": "^2.4.0",
    "react": "17.0.1",
    "react-dev-utils": "^11.0.4",
    "react-dom": "17.0.1",
    "react-hook-form": "^6.15.4",
    "react-native-web": "^0.15.0",
    "react-syntax-highlighter": "^15.4.3",
    "reconnecting-websocket": "^4.4.0",
    "rehype": "^11.0.0",
    "remark": "^13.0.0",
    "remark-autolink-headings": "^6.0.1",
    "remark-code-titles": "^0.1.1",
    "remark-slug": "^6.0.0",
    "stripe": "^8.137.0",
    "twilio": "^3.57.0",
    "websocket": "^1.0.33",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/all-the-cities": "^3.1.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie": "^0.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/fs-extra": "^9.0.8",
    "@types/jest": "^26.0.20",
    "@types/mdx-js__react": "^1.5.3",
    "@types/minimist": "^1.2.1",
    "@types/node": "^14.14.33",
    "@types/node-fetch": "^2.5.8",
    "@types/pg": "^7.14.11",
    "@types/pg-format": "^1.0.1",
    "@types/prompts": "^2.0.9",
    "@types/react": "^17.0.3",
    "@types/react-dev-utils": "^9.0.4",
    "@types/reconnectingwebsocket": "^1.0.6",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-unused-imports": "^1.1.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "prisma": "^2.18.0",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/test/unit",
      "<rootDir>/test/integration"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "test/tsconfig.test.json"
      }
    }
  }
}
