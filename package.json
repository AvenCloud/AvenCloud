{
  "name": "globe",
  "version": "0.0.0-alpha.12",
  "bin": {
    "globe": "./scripts/globe.js"
  },
  "license": "Apache-2.0",
  "scripts": {
    "globe": "./scripts/globe.js",
    "ios": "cd mobile/App && bolt && bolt ios",
    "android": "cd mobile/App && bolt && bolt android",
    "test": "bolt lint && bolt jest",
    "test-update-snapshot": "jest --updateSnapshot",
    "lint": "eslint src",
    "format": "eslint --fix src --parser babel-eslint",
    "web-test": "razzle test --env=jsdom",
    "pre:build": "node scripts/vendorDist.js && bolt format",
    "globe:go-build": "node scripts/build.js",
    "globe:start": "node scripts/start.js",
    "globe:build": "bolt pre:build && bolt globe:go-build",
    "start": "./scripts/globe.js start",
    "build": "./scripts/globe.js build",
    "prod": "NODE_ENV=production && node build/server.js",
    "jest": "jest"
  },
  "bolt": {
    "workspaces": [
      "src/*"
    ]
  },
  "dependencies": {
    "@expo/spawn-async": "^1.3.0",
    "@expo/vector-icons": "^6.3.1",
    "babel-plugin-react-native-web": "^0.8.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-razzle": "^2.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-types": "^6.26.0",
    "clamp": "^1.0.1",
    "create-react-context": "^0.2.2",
    "eslint": "^5.6.0",
    "expo": "^30.0.1",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "glob-to-regexp": "^0.4.0",
    "history": "^4.7.2",
    "hoist-non-react-statics": "^2.5.0",
    "klaw-sync": "^4.0.0",
    "lodash": "^4.17.10",
    "lodash.sortby": "^4.7.0",
    "metro-bundler": "^0.22.1",
    "path-to-regexp": "^1.7.0",
    "prop-types": "^15.6.1",
    "query-string": "^6.1.0",
    "react-art": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-lifecycles-compat": "^3.0.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-30.0.0.tar.gz",
    "react-native-drawer-layout-polyfill": "^2.0.0",
    "react-native-safe-area-view": "^0.7.0",
    "react-native-vector-icons": "^4.6.0",
    "react-native-web": "^0.8.4",
    "react-native-web-vector-icons": "^4.4.3",
    "react-navigation": "https://github.com/react-navigation/react-navigation.git#master",
    "react-navigation-stack": "https://github.com/react-navigation/react-navigation-stack#@ericvicenti/allfiles",
    "react-navigation-transitioner": "https://github.com/react-navigation/react-navigation-transitioner#master",
    "react-syntax-highlighter": "^7.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^22.4.1",
    "babel-preset-react-native": "^2.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "jest-expo": "^28.0.0",
    "lint-staged": "^4.2.1",
    "metro": "^0.45.3",
    "prettier": "^1.13.7",
    "razzle": "^2.4.0",
    "react": "^16.3.1",
    "react-test-renderer": "^16.0.0",
    "webpack": "^4.16.0"
  },
  "peerDependencies": {
    "react": "*"
  },
  "jest": {
    "notify": true,
    "preset": "react-native",
    "testRegex": "./src/.*\\-test\\.js$",
    "setupFiles": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "modulePathIgnorePatterns": [
      "mobile"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|react-navigation-deprecated-tab-navigator)"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
