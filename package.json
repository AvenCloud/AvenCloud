    "history": "^4.7.2",
    "hoist-non-react-statics": "^3.0.1",
    "jest-expo": "^31.0.0",
    "js-yaml": "^3.12.0",
    "json-stable-stringify": "^1.0.1",
    "knex": "^0.15.2",
    "mime": "^2.3.1",
    "minimist": "^1.2.0",
    "objection": "https://github.com/ericvicenti/objection.js.git",
    "path-to-regexp": "^1.7.0",
    "pg": "^7.5.0",
    "pg-format": "^1.0.4",
    "query-string": "^6.2.0",
    "react": "^16.7.0-alpha.0",
    "react-dom": "^16.7.0-alpha.0",
