{
  "name": "cloud",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "a": "yarn add -W",
    "ad": "yarn add -WD",
    "dev": "NODE_ENV=development nodemon --watch server -e js,ts,tsx --exec 'ts-node --project tsconfig.server.json server/server.ts'",
    "build": "yarn prisma generate && yarn build:server && yarn build:next",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "production:build": "rm -rf node_modules && yarn && yarn build",
    "production:migrate": "prisma migrate deploy --preview-feature",
    "production:start": "yarn production:migrate && yarn start",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "start": "NODE_ENV=production node dist/server/server.js",
    "test-integration": "echo comingsoon",
    "test": "yarn test-integration",
    "configure-package": "ts-node --project tsconfig.server.json scripts/configure-package.ts",
    "shell": "yarn configure-package aven-shell && yarn --cwd packages/aven-shell run",
    "client": "yarn configure-package client && yarn --cwd packages/client run",
    "migrate-dev": "DATABASE_URL='postgresql://user:pw@localhost:5992/db' yarn prisma migrate dev --preview-feature"
  },
  "workspaces": [
    "packages/*",
    "rocketship-example"
  ],
  "dependencies": {
    "@chakra-ui/core": "1.0.0-rc.7",
    "@chakra-ui/icons": "^1.0.0-rc.8",
    "@emotion/core": "^10.1.1",
    "@emotion/react": "^11.0.0-rc.0",
    "@emotion/styled": "^11.0.0-rc.0",
    "@expo/spawn-async": "^1.5.0",
    "@prisma/client": "^2.13.0",
    "@sendgrid/mail": "^7.3.0",
    "@smburrows/terraform-npm": "^0.13.5",
    "@stripe/stripe-js": "^1.11.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.8",
    "@types/minio": "^7.0.6",
    "ajv": "^7.0.0-beta.6",
    "all-the-cities": "^3.1.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "emotion-theming": "^10.0.27",
    "expo": "^39.0.3",
    "express": "^4.17.1",
    "fast-json-patch": "https://github.com/Starcounter-Jack/JSON-Patch",
    "framer-motion": "^2.9.4",
    "fs-extra": "^9.0.1",
    "json-schema-to-typescript": "^10.0.0",
    "jwt-simple": "^0.5.6",
    "mdx-prism": "^0.3.1",
    "minimist": "^1.2.5",
    "minio": "^7.0.16",
    "next": "10.0.0",
    "next-mdx-enhanced": "^5.0.0",
    "node-fetch": "^2.6.1",
    "nookies": "^2.5.0",
    "open": "^7.3.0",
    "pg": "^8.5.1",
    "pojo-maps": "^0.3.0",
    "prompts": "^2.4.0",
    "react": "17.0.1",
    "react-dev-utils": "^11.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "^6.8.4",
    "react-native-web": "^0.14.7",
