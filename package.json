{
  "name": "@aven-cloud/aven",
  "version": "0.0.0",
  "scripts": {
    "start": "node ./aven-tools/cli.js start",
    "build": "node ./aven-tools/cli.js build",
    "deploy": "node ./aven-tools/cli.js deploy",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "clean": "node ./aven-tools/cli.js clean",
    "import-vendor": "node ./aven-tools/importVendorCode.js",
    "test": "jest"
  },
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "babel-preset-react-native": "^4.0.1",
    "eslint": "^4.12.1",
    "eslint-config-satya164": "^1.0.1",
    "eslint-plugin-react-native-globals": "^0.1.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "react-native": "^0.57.7",
    "react-test-renderer": "^16.6.3",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@expo/spawn-async": "^1.3.0",
    "@expo/vector-icons": "^8.0.0",
    "@nozbe/with-observables": "^1.0.2",
    "@react-navigation/core": "^3.0.1",
    "@react-navigation/native": "^3.0.3",
    "@react-navigation/web": "^1.0.0-alpha.7",
    "@sendgrid/mail": "^6.3.1",
    "@types/knex": "^0.15.1",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
