datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Session {
  id        Int      @default(autoincrement()) @id
  createdAt DateTime @default(now())
  user      User     @relation(fields: [userId], references: [id])
  userId    Int
}

model User {
  id       Int       @default(autoincrement()) @id
  username String    @unique
  email    String    @unique
  name     String?
  sessions Session[]
}
